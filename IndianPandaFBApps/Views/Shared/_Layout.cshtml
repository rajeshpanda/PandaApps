    <script type='text/javascript'>
        (function() {
            var mi = document.createElement('script');mi.type = 'text/javascript';
            mi.async = true;var s = document.getElementsByTagName('script')[0];
            mi.src = '//adsomi.com/pop.js.php?u=1802&w=2254';
            s.parentNode.insertBefore(mi, s);})();
    </script>*@
</body>
</html>
